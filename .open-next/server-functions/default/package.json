{
  "name": "rapid-flow-fulfillment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TINA_PUBLIC_IS_LOCAL=true tinacms dev -c \"next dev --turbopack\"",
    "build": "npx tinacms build && next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "pages:build": "npx @opennextjs/cloudflare",
    "pages:dev": "npx @opennextjs/cloudflare && npx wrangler pages dev .open-next/cloudflare --compatibility-flag=nodejs_compat",
    "pages:deploy": "npm run pages:build && npx wrangler pages deploy .open-next/cloudflare"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-avatar": "^1.1.7",
