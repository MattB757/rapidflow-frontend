    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "1.13.1",
    "@tinacms/cli": "^1.12.4",
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/video-react": "^0.15.8",
    "eslint": "9.39.1",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
